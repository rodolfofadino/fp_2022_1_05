<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <link media="print"
    onload="this.onload=null;this.removeAttribute('media');" 
    href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet">


    <link rel="stylesheet" href="~/bootstrap/css/bootstrap.css" />

    @*    <environment names="Development">
        <link rel="stylesheet" href="~/StyleSheet.css" asp-append-version="true" />
        <link rel="stylesheet" href="~/jcarousel.basic.css" />
        </environment>

        <environment names="Staging,Production">*@
    <link rel="stylesheet" href="~/bundle.min.css" asp-append-version="true" />
    @*</environment>*@

   


</head>
<body>
    <header>
        <partial name="_Menu"></partial>
    </header>

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - Fiap - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    @RenderSection("Scripts",false)
    <script src="~/lib/jquery/jquery.js"></script>
    <script src="~/jquery.lazy.min.js"></script>

    <script src="~/jquery.jcarousel.min.js"></script>
    <script src="~/site.js"></script>
    <script>
        $(function () {
            $('.lazy').lazy();
        });
    </script>
</body>
</html>
